package aprender.pkgfor;

import java.util.Scanner;

public class AprenderFor {
    public static void main(String[] args) {
    Scanner ler = new Scanner (System.in);
    int tabuada;
    int soma;
    
        System.out.println("Digite uma tabuada que gostaria de saber: ");
        tabuada = ler.nextInt();
        
            
    for (int  i = 0; i < 11; i++){
        soma = tabuada * i;
        System.out.println("resultado de " + tabuada + " x " + i +" Ã©: " + soma);
        
    }
}
}
